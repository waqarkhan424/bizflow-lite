
generator client {
  provider = "prisma-client"
}

datasource db {
  provider = "mongodb"
}


model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String?
  email     String   @unique
  password  String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}